{
  "expo": {
    "name": "InflamEase",
    "slug": "inflamease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "inflamease",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF6B5A"
      },
      "package": "com.khan_hmza4.inflamease"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FF6B5A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "displayName": "InflamEase",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e1cf4669-cb86-4be8-9309-c615908b99ec"
      }
    }
  }
}


